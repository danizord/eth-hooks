{
  "name": "eth-hooks",
  "version": "4.0.44",
  "description": "A set of hooks to turbocharge buidling",
  "author": "Shravan Sunder<shravan.sunder.dev@gmail.com>",
  "repository": "https://github.com/scaffold-eth/eth-hooks.git",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    {
      "name": "Austin Griffith",
      "url": "https://github.com/austintgriffith"
    },
    {
      "name": "Shravan Sunder",
      "url": "https://github.com/ShravanSunder"
    }
  ],
  "scripts": {
    "watch": "tsc -w --noEmit",
    "clean": "shx rm -rf ./lib",
    "build:old": "yarn clean && ts-patch i && yarn shx mkdir lib && yarn shx cp -r src lib/src.ts && tsc -p tsconfig.build.json",
    "build": "yarn clean && node esbuild.config.mjs && ts-patch i -s && tsc -p tsconfig.build.json",
    "package": "shx cp -R package.json ./lib && shx cp -R README.md ./lib",
    "publish:pre": "yarn clean && yarn build && yarn package",
    "publish:yalc": "yarn build && yarn package && cd lib && yalc publish --no-script --push",
    "publish:next": "yarn build && yarn package && cd lib && npm publish --tag next",
    "publish:latest": "yarn build && yarn package && cd lib && npm publish --tag latest",
    "docs:api": "yarn typedoc --options ./typedoc.json && shx cp -R README.md ./docusaurus/docs/overview.md",
    "lint": "eslint --config ./.eslintrc --ignore-path ./.eslintignore ./src/**/*.(ts|tsx) ./test/**/*.(ts|tsx)",
    "lint:report": "eslint --config ./.eslintrc --ignore-path ./.eslintignore --output-file eslint_report.json --format json ./src/**/*.(ts|tsx)",
    "format": "yarn prettier -w ./src ./test ./test-files",
    "format:check": "yarn prettier -c ./src ./test ./test-files",
    "test": "yarn test:hardhat",
    "test:hardhat": "yarn hh:run test  --network hardhat",
    "test:mocha": "yarn hh:env yarn mocha --config .mocharc.js",
    "hh:run": "yarn hh:env yarn hardhat",
    "hh:chain": "yarn hh:run node --no-deploy",
    "hh:compile": "yarn hh:run compile",
    "hh:env": "cross-env TS_NODE_PROJECT=tsconfig.testing.json TS_NODE_TRANSPILE_ONLY=1",
    "prepare": "cd .. && yarn husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-provider": "^5.5.1",
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/networks": "^5.5.2",
    "@ethersproject/providers": "^5.5.3",
    "@ethersproject/signing-key": "^5.5.0",
    "@ethersproject/solidity": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@uniswap/token-lists": "^1.0.0-beta.27",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/types": "^6.0.7",
    "axios": "^0.25.0",
    "ethers": "^5.5.4",
    "lodash.isequal": "^4.5.0",
    "merge-anything": "^5.0.2",
    "react-query": "^3.34.16",
    "ts-invariant": "^0.9.4",
    "use-debounce": "^7.0.1",
    "usehooks-ts": "^2.2.2",
    "web3modal": "^1.9.5"
  },
  "peerDependencies": {
    "@uniswap/sdk": "^3.0.3",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@atixlabs/hardhat-time-n-mine": "^0.0.5",
    "@defi-wonderland/smock": "^2.0.7",
    "@ethereum-waffle/ens": "^3.3.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@openzeppelin/contracts": "^4.5.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.25",
    "@types/pretty-time": "^1.1.2",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.39",
    "@types/react-blockies": "^1.4.1",
    "@types/react-dom": "^17.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@uniswap/sdk": "^3.0.3",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "earljs": "^0.2.1",
    "esbuild": "^0.14.23",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ethereum-waffle": "^3.4.0",
    "global-jsdom": "^8.4.0",
    "hardhat": "^2.8.3",
    "hardhat-deploy": "^0.9.28",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.1",
    "mocha-github-actions-reporter": "^0.2.4",
    "mocha-junit-reporter": "^2.0.2",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shx": "^0.3.4",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.5.0",
    "ts-patch": "^1.4.5",
    "ts-sinon": "^2.0.2",
    "tsconfig-paths": "^3.12.0",
    "typechain": "^7.0.0",
    "typedoc": "^0.22.12",
    "typedoc-plugin-markdown": "^3.11.14",
    "typedoc-plugin-mermaid": "^1.6.0",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "react",
    "create-eth-app",
    "typescript"
  ],
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "volta": {
    "node": "16.13.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "yarn lint --fix",
      "yarn format"
    ],
    "**/*.{json}": [
      "yarn format"
    ]
  }
}
